            },
            {
                "paragraph": {
                    "string": "Thank you for talking with me on {cmr_date} about your health and medications.",
                    "string_replacements": {
                        "molina": {
                            "Spanish": "Gracias por hablar conmigo el {cmr_date} sobre su salud y sus medicamentos."
