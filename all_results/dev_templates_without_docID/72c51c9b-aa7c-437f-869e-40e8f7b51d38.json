        "document_content": [
            {
                "paragraph": {
                    "string": "Dear {patient_full_name},",
                    "text_style": {
                        "parent_style": "ParagraphText",
                        "spaceBefore": 24
                    }
                }
            },
            {
                "paragraph": {
                    "string": "We are offering a medication management program at no extra cost to help United Healthcare members like you improve your health. You have been selected for this program to make sure you are on safe and effective medicines for your health conditions. Our experienced pharmacists will help you achieve the following:",
                    "text_style": {
                        "parent_style": "ParagraphText",
                        "spaceBefore": 12
                    }
                }
            },
            {
                "list": {
                    "items": [
                        "<b>Better Health</b><br/>A trained pharmacist will help you achieve great benefit from your medicines by making sure you are on the right medicines for your specific health conditions.",
                        "<b>Increased safety</b><br/>Some medicines may cause side effects or may not work well when taken incorrectly or with other medicines. The pharmacist will make sure that you are taking the right combination of medicines and also provide tips on how to manage any side effects.",
                        "<b>Lower costs</b><br/>The pharmacist can also look for ways to save you money by recommending lower cost options or removing any unnecessary medicines."
                    ],
                    "list_attributes": {
                        "bulletFontSize": 14,
                        "bulletType": "bullet",
                        "leftIndent": 24,
                        "spaceAfter": 12,
                        "spaceBefore": 12
                    },
                    "list_item_attributes": {
                        "leftIndent": 36
                    },
                    "text_style": {
                        "parent_style": "ParagraphText"
                    }
                }
            },
            {
                "paragraph": {
                    "string": "Take advantage of this program at no extra cost by calling for more information or to speak to a pharmacist directly toll-free at <b>1-800-XXX-XXXX</b> any time M-F from 8a-6p CT.",
                    "text_style": {
                        "parent_style": "ParagraphText"
                    }
                }
            },
